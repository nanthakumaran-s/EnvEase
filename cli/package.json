{
  "name": "envease-cli",
  "version": "0.0.1",
  "description": "",
  "main": "./bin/cli.js",
  "type": "module",
  "bin": {
    "envease": "bin/cli.js"
  },
  "scripts": {
    "dev": "node ./bin/cli.js",
    "transcompile": "esbuild --platform=node --bundle --outfile=build/envease.js bin/cli.js",
    "pack": "npm-run-all --parallel package*",
    "package-win-x64": "pkg -t node16-win-x64 -o out/envease-win-x64 build/envease.js",
    "package-win-arm64": "pkg -t node16-win-arm64 -o out/envease-win-arm64 build/envease.js",
    "package-lin-x64": "pkg -t node16-linux-x64 -o out/envease-linux-x64 build/envease.js",
    "package-lin-arm64": "pkg -t node16-linux-arm64 -o out/envease-linux-arm64 build/envease.js",
    "package-mac-x64": "pkg -t node16-macos-x64 -o out/envease-macos-x64 build/envease.js",
    "package-mac-arm64": "pkg -t node16-macos-arm64 -o out/envease-macos-arm64 build/envease.js"

  },
  "author": "Nanthakumaran S <nanthankumaransenthil@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "chalk": "^5.3.0",
    "clear": "^0.1.0",
    "configstore": "^6.0.0",
    "inquirer": "^9.2.7",
    "ora": "^6.3.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}
